<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.frutosajniahperez.libreria">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".PrincipalProfesor"
            android:label="@string/title_activity_principal_profesor"
            android:windowSoftInputMode="adjustPan" />
        <activity android:name=".RegistroProfesor" />
        <activity android:name=".IniciarSesion" />
        <activity android:name=".ModificarColegio" />
        <activity android:name=".RegistroColegio" />
        <activity android:name=".RegistroAdministrador" />
        <activity android:name=".EleccionRegistro" />
        <activity android:name=".Principal">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>